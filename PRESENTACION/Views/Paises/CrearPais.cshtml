@model ENTIDAD.PaisE

@{
    ViewBag.Title = "CrearPais";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Crear Pais</h2>

<table border="0">
    @using (Html.BeginForm("AgregarPais", "Paises", FormMethod.Post))
     {
        <tr>
            <td>Nombre pais  </td>
            <td><input type="text" name="txtNombre" /></td>
        </tr>

        <tr>
            <td><button type="submit">Agregar</button></td>
        </tr>
     }
</table>

